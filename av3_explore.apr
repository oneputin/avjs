(SEd.58
	Name:	"SEdit.GlobalVar"
	CreationDate:	"Mittwoch, 20. Januar 2016 11:45:41"
	GUIName:	"Script"
	Win:	59
	CSMgr:	60
	DocExts:	61
	Source:	"' Name: SEdit.GlobalVar     : Wed Aug 29 11:17:10 2012 : tools_sedit.apr\n' Init Global Variable if not existing\n' _global = \"_global\"\n\ndefValue = NIL\n\nparams = NIL\nif (self.is(list)) then \n  params = self\n  standalone=false\nelse\n  ' params = _xSGV\n  standalone=true\nend  \n\nif (params<>NIL) then \n  varName  = params.get(0)\n  if (params.count>1) then  defValue = params.get(1) end\n  _xSGV = params \nelse  \n  varName  = msgbox.input(\"Enter a VariableName\",\"Sedit.GlobalVar\", _global.asstring)\nend\n\nif (varName=NIL)
 then  return NIL  end\n\n_global = varName\n\n' 1.  TEST- und SET- Function \n\ntestScript = Script.Make(\"x=\"+varName+\" return x\")\n\nif (testScript.HasError) then  \n\n  if (standalone) then \n    if (msgbox.yesno(\"Variable doesnt exist. Initiate the variable now?\",\"Sedit.GlobalVar\",true).not) then  \n    return nil  \n  end\n\n  defValue = msgbox.input(\"Enter Initial Value.\",\"GloVar\",\"Sedit.GlobalVar\")\n  if (defValue.is(string)) then \n    defValue=defValue.quote  \n  else  \n    defValue=defValue.asstring  \n  end\n\n  setS
cript = Script.Make(varName+\"=\"+defValue+\" X=\"+varName+\"  return x\")\n\n  gloVarValue = setScript.doit(NIL)\n  ' * * * * * * * * * * * * * * *\n  addon = nl+\"value reset !\"\n  end\n\nelse\n\n  gloVarValue = testScript.doit(NIL)\n  ' * * * * * * * * * * * * * * * * * \n  addon = \"\"\nend\n\nif (standalone.NOT) then\n  return   gloVarValue\nend	\n\n\n' 2. VIEW - Function(s) (im standalone-Mode)\n\n' 2.1 \n\nif (glovarValue.is(FileName)) then \n  if (file.exists(glovarValue)) then \n    if (msgbox.yesno(\"View content of file ?\"+nl+gloV
arValue.asstring.quote,\"Sedit.GlobalVar\",true)) then \n\n      ext = gloVarValue.getExtension\n\n      if (ext=\"dbf\") then \n        oldTable = av.findDoc(\"GlobalVarViewer\")\n        if (oldTable<>NIL) then \n        av.getProject.remove(oldTable)  \n        end\n\n        tstVtab = VTab.Make(gloVarValue,false,false)\n        tstTable = Table.Make(tstVTab)\n        tstTable.getWin.Open\n        tstTable.setName(\"GlobalVarViewer\")\n\n      elseif (ext=\"xml\") then \n        av.run(\"AV.XMLProcessor\", {gloVarValue.asstring})\n 
       ' -----------------------	\n\n      else \n      system.execute(\"explorer.exe\"++gloVarValue.asstring.quote)\n      ' * * * * * * * *\n      end\n      return NIL \n    end  \n  end  \nend\n\n' 2.2 \n\nif (glovarValue.is(Collection).NOT) then \n  msgbox.report(\"Value of GlobalVar \"+varName.asstring.quote+nl+nl+gloVarValue.asstring+addon, \"Sedit.GlobalVar\")  \n  return NIL \nend\n\n\n' 2.3  Compile viewing options and select one \n\nxmlMode  = NIL\nsortMode = NIL  \nviewModes = {}\n\nif (av.findScript(\"SXML.test\")<>NIL) then \n
  t = av.run(\"SXML.test\",{glovarValue})\n  if (t <> NIL) then  \n    viewModes.add(\"sxml\")\n  end  \nend\n\n\nif ( glovarValue.is(Dictionary)) then \n  if (av.findScript(\"AV.XMLProcessor\")<>NIL) then    ' original: \"AV.XMLview\" \n    viewModes.add(\"xml\")\n  end\nend\n\nif (av.findScript(\"L\") <> NIL) then\n  viewModes.add(\"original\")\n  viewModes.add(\"sorted alpha\")\n  viewModes.add(\"sorted num\")\nend    \n\nif (viewModes.count > 1) then \n  viewMode = msgbox.listasstring(viewModes, \"Select a mode to view \"+varName.asstring++\"!
\", \"Sedit.GlobalVar\")\n  if (viewMode=NIL) then  return NIL  end\nelse\n  viewMode = NIL \nend\n\n\n' 2.4 Viewing execution \n\nif (viewMode = \"sxml\") then \n\nn = glovarValue.count\nif ( n > 1) then \n  attDic=dictionary.make(3)\n  attdic.set(\"count\", n.asstring)\n  glovarValue = av.run(\"SXML.Node\",{\"sxmlviewer\", glovarValue, attDic})    \n  end  \n  av.run(\"AV.XMLProcessor\", {gloVarValue })\n  ' ------------------------  \n\nelseif (viewMode = \"xml\") then \n\n  if (glovarValue.count > 1) then \n    klammer = dictionary.make(3)\n
    klammer.set(\"avdict\", glovarValue)  \n    glovarValue = klammer \n  end  \n\n  av.run(\"L\",{gloVarValue, \"xml of:\"++varName.asstring, \"xml\"})\n\nelseif (viewMode = \"sorted num\") then \n\n  av.run(\"L\",{gloVarValue, \"sorted:\"++varName.asstring})\n\nelseif (viewMode = \"sorted alpha\") then \n\n  av.run(\"L\",{gloVarValue, \"sorted:\"++varName.asstring, TRUE })\n\nelseif (viewMode = \"original\") then \n\n  av.run(\"L\",{gloVarValue, \"original:\"++varName.asstring })\n  ' ----------\nelse\n\nmsgbox.report(varName ++\"is a Collection with\"
++gloVarValue.count.asstring++\"elements.\",\"Sedit.GlobalVar\")\n\nend\n"
)

